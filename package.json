{
  "name": "poker-9max-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "concurrently -k -n server,web \"npm run dev -w @poker/server\" \"npm run dev -w @poker/web\"",
    "build": "npm run build -ws --if-present",
    "test": "npm run test -ws --if-present"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "typescript": "^5.7.3"
  }
}
